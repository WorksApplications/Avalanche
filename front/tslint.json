{
  "rulesDirectory": ["tslint-plugin-prettier"],
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "prettier": true,
    "object-literal-sort-keys": false
  },
  "linterOptions": {
    "exclude": ["src/generated/**/*"]
  },
  "jsRules": {
    "prettier": true,
    "object-literal-sort-keys": false
  }
}
